class Solution {
    public int triangularSum(int[] nums) {
        if(nums.length == 1){
            return nums[0];
        }

        for(int i=0; i<nums.length-1; i++){
            nums[i] = (nums[i] + nums[i+1])%10;
        }

        return triangularSum(Arrays.copyOfRange(nums, 0, nums.length-1));
    }
}